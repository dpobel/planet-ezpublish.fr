{% for post in posts %}
    {{ render(
        controller(
            "ez_content:viewLocation",
            {'locationId': post.contentInfo.mainLocationId, 'viewType': viewType}
        ),
        {'strategy': 'esi'}
    ) }}
{% endfor %}

{% if navigator %}
    {% include "PlanetBundle::navigator.html.twig" with
        {'total': total, 'url': path( paginationRoot ), 'offset': offset, 'limit': planet.page.posts}
    %}
{% endif %}
