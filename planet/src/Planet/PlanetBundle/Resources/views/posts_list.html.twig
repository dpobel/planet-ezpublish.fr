{% for post in posts %}
    {% render "ez_content:viewLocation" with
        {'locationId': post.contentInfo.mainLocationId, 'viewType': viewType},
        {'standalone': true}
    %}
{% endfor %}

{% if navigator %}
    {% include "PlanetBundle::navigator.html.twig" with
        {'total': total, 'url': path( root ), 'offset': offset, 'limit': planet.page.posts}
    %}
{% endif %}
